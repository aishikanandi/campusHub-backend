{
    "version": 2,
    "builds": [
        {
            "src": "campus_hub/app.py",
            "use": "@vercel/python",
            "config": {
                "pythonVersion": "3.11",
                "poetry": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "campus_hub/app.py"
        }
    ]
}

